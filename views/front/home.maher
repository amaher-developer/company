<script src="/front/js/slick.min.js"></script>
<script type="text/javascript">
    $(document).ready(function(){
        $('.slider-for').slick({
            slidesToShow: 1,
            slidesToScroll: 1,
            arrows: false,
            fade: false,

            asNavFor: '.slider-nav'
        });






        $('.slider-nav').slick({
            centerMode: true,
            asNavFor: '.slider-for',
            slidesToShow: 5,
            arrows: false,
            focusOnSelect: true,
            slidesToScroll: 1,
            centerPadding: '0',
            responsive: [
                {
                    breakpoint: 992,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 1,
                        infinite: true,
                        dots: true
                    }
                },
                {
                    breakpoint: 500,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 1
                    }
                }


            ]





        });




        $('.slider.slider-nav').removeClass('slick-active');


        $('.slider.slider-nav').eq(0).addClass('slick-active');


        $('.slider').on('beforeChange', function (event, slick, currentSlide, nextSlide) {
            var mySlideNumber = nextSlide;
            $('.slider.slider-nav').removeClass('slick-active');
            $('.slider.slider-nav').eq(mySlideNumber).addClass('slick-active');
        });



        $('.slider').on('afterChange', function(event, slick, currentSlide){
            $('.content').hide();
            $
            ('.content[data-id=' + (currentSlide + 1) + ']').show();
        });







        $(".read-more").click(function(){



            $(".img-tab-2").addClass('img-animation-tab-2');
            $(".img-tab-2").removeClass('img-animation-tab-2-of');
            $(".img-tab-1").addClass('img-animation-tab-1-of');


            $(".back-tab-2").addClass('title-animation-tab-2');
            $(".back-tab-2").removeClass('title-animation-tab-2-of');
            $(".tab-2").addClass('animation-tab-2');
            $(".tab-2").removeClass('animation-tab-2-of');
            $(".tab-1-title").addClass('title-animation-tab-1');
            $(".tab-1-title").removeClass('title-animation-tab-1-of');
            $(".tab-1").addClass('animation-tab-1');
            $(".tab-1").removeClass('animation-tab-1-of');
            $(".tab-2").addClass('show-me');
            $(".tab-2").removeClass('show-me-of');



        });

        $(".back-tab-2").click(function(){
            $(".img-tab-1").addClass('img-animation-tab-1');
            $(".img-tab-2").removeClass('img-animation-tab-2');
            $(".img-tab-2").addClass('img-animation-tab-2-of');
            $(".img-tab-1").removeClass('img-animation-tab-1-of');


            $(".tab-1-title").removeClass('title-animation-tab-1');
            $(".tab-1-title").addClass('title-animation-tab-1-of');
            $(".tab-1").removeClass('animation-tab-1');
            $(".tab-1").addClass('animation-tab-1-of');
            $(".tab-2").removeClass('show-me');
            $(".tab-2").addClass('show-me-of');
            $(".back-tab-2").removeClass('title-animation-tab-2');
            $(".back-tab-2").addClass('title-animation-tab-2-of');
            $(".tab-2").removeClass('animation-tab-2');
            $(".tab-2").addClass('animation-tab-2-of');



        });



        $(".down-me-2").click(function(){
            $(".tab-3").addClass('showup2');
            $(".tab-3").removeClass('showup2-of');
            $(".tab-2").addClass('hide-2');
            $(".go-back-2").addClass('go-back-2-in');






        });



        $(".go-back-2").click(function(){
            $(".tab-3").addClass('showup2-of');
            $(".tab-2").removeClass('hide-2');
            $(".tab-3").addClass('showup2-of');


        });

    });

</script>


</body>
</html>